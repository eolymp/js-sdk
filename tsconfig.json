{
  "compilerOptions": {
    "declaration": true,
    "strictNullChecks": true,
    "target": "es6",
    "module": "commonjs",
    "isolatedModules": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true
  },
  "exclude": ["node_modules"]
}
